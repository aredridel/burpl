{
  "name": "burpl",
  "version": "1.3.0",
  "description": "Control your daemons with tab-completable repls",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aredridel/burpl.git"
  },
  "bin": {
    "burpl": "cli.js"
  },
  "keywords": [
    "repl",
    "cli",
    "executive"
  ],
  "author": "Aria Stewart <aredridel@dinhe.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aredridel/burpl/issues"
  },
  "homepage": "https://github.com/aredridel/burpl#readme",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "valid-typeof": "error",
      "no-unused-vars": "error",
      "no-undef": "error"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  },
  "dependencies": {
    "common-prefix": "^1.1.0",
    "minimist": "^1.2.0",
    "multiplex-rpc": "^1.0.1",
    "pump": "^1.0.3",
    "readline-ext": "^1.2.0"
  },
  "devDependencies": {
    "unix-listen": "^0.1.1"
  }
}
